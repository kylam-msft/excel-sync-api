{
  "extends": ["tslint:recommended"],
  "rules": {
    "arrow-parens": false,
    "interface-name": false,
    "no-console": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "variable-name": false
  }
}
